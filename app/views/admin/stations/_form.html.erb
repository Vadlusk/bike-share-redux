<%= form_for [:admin, @station] do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name %>

  <%= f.label :dock_count  %>
  <%= f.number_field :dock_count %>

  <%= f.label :city  %>
  <%= f.text_field :city %>

  <%= f.label :installation_date  %>
  <%= f.date_field :installation_date %>

  <%= f.label :lat  %>
  <%= f.number_field  :lat %>

  <%= f.label :long  %>
  <%= f.number_field  :long %>

  <div class="cancel-submit">
    <%= link_to "Cancel", stations_path, class: 'btn btn-outline-light' %>
    <%= f.submit class: 'btn btn-outline-light' %>
  </div>
<% end %>
